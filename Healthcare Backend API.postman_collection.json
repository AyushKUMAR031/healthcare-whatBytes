					"name": "Register User",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Generate random values using Postman's dynamic variables\r",
									"let email = pm.variables.replaceIn('{{$randomEmail}}');\r",
									"let fullName = pm.variables.replaceIn('{{$randomFullName}}');\r",
									"let password = pm.variables.replaceIn('{{$randomPassword}}');\r",
									"\r",
									"\r",
									"// Set the values as collection variables\r",
									"pm.collectionVariables.set('email', email);\r",
									"pm.collectionVariables.set('fullName', fullName);\r",
									"pm.collectionVariables.set('password', password);"
